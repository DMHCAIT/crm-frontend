{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd crm-frontend-main && npm ci && npm run build",
  "devCommand": "cd crm-frontend-main && npm run dev",
  "installCommand": "cd crm-frontend-main && npm ci",
  "outputDirectory": "crm-frontend-main/dist",
  "framework": "vite",

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}