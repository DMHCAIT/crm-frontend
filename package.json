{
  "name": "dmhca-crm-frontend",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "description": "DMHCA CRM Frontend with Production Authentication - FIXED VERSION",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "env-check": "echo \"✅ .env file found\" && echo \"Environment variables ready for Vite (accessible via import.meta.env in browser)\"",
    "clear-data": "node -e \"localStorage.clear(); console.log('✅ Browser data cleared');\"",
    "start": "npm run dev",
    "serve": "npm run build && npm run preview",
    "deploy": "npm run build && npx vercel",
    "deploy-production": "npm run build && npx vercel --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "axios": "^1.12.0",
    "facebook-nodejs-business-sdk": "^23.0.1",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0"
  },
  "engines": {
    "node": ">=18.x"
  }
}
